{
  "browserSyncOptions" : {
    "proxy": "<your-domain-here>",
    "notify": true
  },
  "browserSyncWatchFiles" : [
    "./css/*.min.css",
    "./js/*.min.js",
    "./**/*.php"
  ],
  "paths" : {
    "js": "./js",
    "css": "./css",
    "img": "./img",
    "svg": "svg",
		"imgsrc": "./src/img",
    "sass": "./sass",
    "node": "./node_modules/",
    "bower": "./bower_components/",
    "dev": "./src",
    "dist": "./dist",
    "distprod": "./dist-product",
    "vendor": ""
  }
}
